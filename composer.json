{
    "name": "app-hive/toml",
    "description": "Toml parser for PHP",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Philo Hermans",
            "email": "me@philohermans.com"
        }
    ],
    "require": {
        "php": "^8.3"
    },
    "require-dev": {
        "laravel/pint": "^1.27",
        "pestphp/pest": "^3.0",
        "phpstan/phpstan": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "AppHive\\Toml\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AppHive\\Toml\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "@phpstan",
            "@pest"
        ],
        "phpstan": "phpstan analyse",
        "pest": "pest"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
